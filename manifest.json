{
    "name": "RottenFlix",
    "description": "A chrome extension for adding rotten tomatoes ratings to movies/tv shows",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://www.netflix.com/*"],
          "css": [ "css/RottenFlix.css" ],
          "js": ["scripts/content.js"]
       
        }
      ]
  }

